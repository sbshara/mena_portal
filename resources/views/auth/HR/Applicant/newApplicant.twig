                                    <label for="attachment1">File: &nbsp;</label>
                                    <input type="file" id="attachment1" name="attachment[]"> &nbsp;
                                </div>
                                <div class="input-group inline has-feedback{{ errors.attachmentCountry1 ? ' has-error' : '' }}" id="attachmentCountry1">
